{
    "name": "Plex",
    "maintainer": "Plex Inc.",
    "url": {
        "docker-hub": "https://hub.docker.com/r/plexinc/pms-docker/"
    },
    "description": "Plex Media Server organizes your video, music, and photo collections and streams them to all of your screens.",
    "webui": {
        "enable": true,
        "container-port": 32400,
        "ssl": false,
        "path": "/web"
    },
    "config": {
        "repository": "plexinc/pms-docker",
        "tag": "latest",
        "command": [],
        "env": [
            {
                "name": "TZ",
                "type": "str",
                "value": "",
                "description": "Timezone (https://en.wikipedia.org/wiki/List_of_tz_database_time_zones#List)",
                "editable": true,
                "regexrules": [
                    {
                        "regex": "[A-Za-z]+/[A-Za-z_]+",
                        "message": "Invalid Timezone"
                    }
                ]
            },
            {
                "name": "PLEX_CLAIM",
                "type": "str",
                "value": "",
                "description": "Plex Claim Token (https://www.plex.tv/claim/)",
                "editable": true,
                "regexrules": [
                    {
                        "regex": "^claim-",
                        "message": "Invalid Plex Claim Token"
                    }
                ]
            }
        ],
        "volumes": [
            {
                "value": "/home/plex/database",
                "bind": "/config",
                "mode": "rw",
                "description": "Plex Config Folder",
                "editable": true
            },
            {
                "value": "/home/plex/transcode",
                "bind": "/transcode",
                "mode": "rw",
                "description": "Plex Transcode Folder",
                "editable": true
            },
            {
                "value": "/home/plex/media",
                "bind": "/data",
                "mode": "rw",
                "description": "Plex Data Folder",
                "editable": true
            }
        ],
        "ports": [
            {
                "value": 32400,
                "bind": 32400,
                "type": "tcp",
                "description": "Plex webui"
            },
            {
                "value": 3005,
                "bind": 3005,
                "type": "tcp",
                "description": ""
            },
            {
                "value": 8324,
                "bind": 8324,
                "type": "tcp",
                "description": ""
            },
            {
                "value": 32469,
                "bind": 32469,
                "type": "tcp",
                "description": ""
            },
            {
                "value": 1900,
                "bind": 1900,
                "type": "udp",
                "description": ""
            },
            {
                "value": 32410,
                "bind": 32410,
                "type": "udp",
                "description": ""
            },
            {
                "value": 32412,
                "bind": 32412,
                "type": "udp",
                "description": "",
                "editable": false
            },
            {
                "value": 32413,
                "bind": 32413,
                "type": "udp",
                "description": ""
            },
            {
                "value": 32414,
                "bind": 32414,
                "type": "udp",
                "description": ""
            }
        ]
    }
}