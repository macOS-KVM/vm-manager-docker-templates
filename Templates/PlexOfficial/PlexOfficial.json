{
    "name": "Plex Official",
    "maintainer": "Plex Inc.",
    "repository": "plexinc/pms-docker",
    "tag": "latest",
    "docker-hub-url": "https://hub.docker.com/r/plexinc/pms-docker/",
    "description": "Plex Media Server organizes your video, music, and photo collections and streams them to all of your screens.",
    "webui": {
        "enable": true,
        "container-port": 32400,
        "ssl": false
    },
    "command": [],
    "env": [
        {
            "name": "TZ",
            "type": "str",
            "default": "Europe/Brussels",
            "description": "Timezone (https://en.wikipedia.org/wiki/List_of_tz_database_time_zones#List)",
            "editable": true
        },
        {
            "name": "PLEX_CLAIM",
            "type": "str",
            "default": "claim-xxxxxxxxxxxxxxxxxxxx",
            "description": "Plex Claim Token (https://www.plex.tv/claim/)",
            "editable": true
        }
    ],
    "volumes": [
        {
            "default": "/home/plex/config",
            "bind": "/config",
            "mode": "rw",
            "description": "Plex Config Folder",
            "editable": true
        },
        {
            "default": "/home/plex/transcde",
            "bind": "/transcode",
            "mode": "rw",
            "description": "Plex Transcode Folder",
            "editable": true
        },
        {
            "default": "/home/plex/data",
            "bind": "/data",
            "mode": "rw",
            "description": "Plex Data Folder",
            "editable": true
        }
    ],
    "ports": [
        {
            "default": 32400,
            "bind": 32400,
            "type": "tcp",
            "description": "Plex webui",
            "editable": false
        },
        {
            "default": 3005,
            "bind": 3005,
            "type": "tcp",
            "description": "",
            "editable": false
        },
        {
            "default": 8324,
            "bind": 8324,
            "type": "tcp",
            "description": "",
            "editable": false
        },
        {
            "default": 32469,
            "bind": 32469,
            "type": "tcp",
            "description": "",
            "editable": false
        },
        {
            "default": 1900,
            "bind": 1900,
            "type": "udp",
            "description": "",
            "editable": false
        },
        {
            "default": 32410,
            "bind": 32410,
            "type": "udp",
            "description": "",
            "editable": false
        },
        {
            "default": 32412,
            "bind": 32412,
            "type": "udp",
            "description": "",
            "editable": false
        },
        {
            "default": 32413,
            "bind": 32413,
            "type": "udp",
            "description": "",
            "editable": false
        },
        {
            "default": 32414,
            "bind": 32414,
            "type": "udp",
            "description": "",
            "editable": false
        }
    ]
}
